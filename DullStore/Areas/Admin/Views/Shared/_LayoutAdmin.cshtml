@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quản trị viên</title>
    <link href="~/Areas/Admin/ContentAd/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Areas/Admin/ContentAd/vendor/metisMenu/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Areas/Admin/ContentAd/dist/css/sb-admin-2.css" rel="stylesheet" />
    <link href="~/Areas/Admin/ContentAd/vendor/morrisjs/morris.css" rel="stylesheet" />
    <link href="~/Areas/Admin/ContentAd/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link rel="shortcut icon" href="~/Areas/Admin/ContentAd/ico/admin.ico" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"
          integrity="sha512-HK5fgLBL+xu6dm/Ii3z4xhlSUyZgTT9tuc/hSrtw6uzJOvgRr2a9jyxxT1ely+B+xFAmJKVSTbpM/CuL7qxO8w=="
          crossorigin="anonymous" />
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&family=Roboto:wght@300;400;500;700&display=swap"
          rel="stylesheet">
</head>
<body style="background: linear-gradient(to right, hsl(200deg 19% 76%),hsl(191deg 58% 79%), hsl(154deg 44% 68%));">
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="background-color: #117A65;">

            <!-- /.navbar-header -->
            <!-- /.navbar-top-links -->
            @RenderPage("_MenuAdmin.cshtml")
            <!-- /.navbar-static-side -->
        </nav>
        <div id="page-wrapper" style="background: linear-gradient(to right, hsl(204, 8%, 90%), hsl(184, 6%, 91%), hsl(210, 9%, 92%)); ">
            @if (TempData["AlertMessage"] != null)
            {
                <div class="form-group">
                    <div id="AlertBox" class="alert @TempData["AlertType"] hide">
                        @TempData["AlertMessage"]
                    </div>
                </div>
            }

            @RenderBody()
        </div>
        <!-- /#page-wrapper -->
    </div>
    <script src="~/Areas/Admin/ContentAd/vendor/jquery/jquery.min.js"></script>
    <script src="~/Areas/Admin/ContentAd/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Areas/Admin/ContentAd/vendor/metisMenu/metisMenu.min.js"></script>
    <script src="~/Areas/Admin/ContentAd/vendor/raphael/raphael.min.js"></script>
    <script src="~/Areas/Admin/ContentAd/vendor/morrisjs/morris.min.js"></script>
    <script src="~/Areas/Admin/ContentAd/data/morris-data.js"></script>
    <script src="~/Areas/Admin/ContentAd/js/sb-admin-2.js"></script>
    <script>
        $(function () {
            $("#AlertBox").removeClass("hide");
            $("#AlertBox").delay("2000").slideUp("500");
        });
    </script>
</body>
</html>